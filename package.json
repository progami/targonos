{
  "name": "targon",
  "private": true,
  "version": "1.1.0",
  "packageManager": "pnpm@10.21.0",
  "scripts": {
    "predev": "node scripts/prepare-dev-logs.js",
    "dev": "turbo run dev --parallel",
    "build": "node scripts/run-turbo-task.js build",
    "test": "node scripts/run-turbo-task.js test",
    "lint": "node scripts/run-turbo-task.js lint",
    "typecheck": "node scripts/run-turbo-task.js type-check",
    "format": "node scripts/run-turbo-task.js format",
    "refresh:talos-dev": "bash scripts/refresh-dev-app.sh talos",
    "refresh:targonos-dev": "bash scripts/refresh-dev-app.sh sso",
    "refresh:website-dev": "bash scripts/refresh-dev-app.sh website",
    "refresh:kairos-dev": "bash scripts/refresh-dev-app.sh kairos",
    "refresh:xplan-dev": "bash scripts/refresh-dev-app.sh xplan",
    "refresh:atlas-dev": "bash scripts/refresh-dev-app.sh atlas",
    "refresh:plutus-dev": "bash scripts/refresh-dev-app.sh plutus",
    "refresh:hermes-dev": "bash scripts/refresh-dev-app.sh hermes",
    "deploy:metadata": "bash scripts/update-deploy-metadata.sh"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "playwright": "^1.57.0",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.1.10",
      "@types/react-dom": "19.1.6"
    },
    "packageExtensions": {
      "next@16.0.8": {
        "dependencies": {
          "@types/react": "19.1.10",
          "@types/react-dom": "19.1.6"
        }
      }
    }
  }
}
