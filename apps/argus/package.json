{
  "name": "@targon/argus",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3016",
    "clean": "rm -rf .next out dist",
    "build": "prisma generate --schema prisma/schema.prisma && next build",
    "start": "next start",
    "lint": "eslint app components lib",
    "type-check": "prisma generate --schema prisma/schema.prisma && tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "prisma:generate": "prisma generate --schema prisma/schema.prisma",
    "prisma:migrate:dev": "prisma migrate dev --schema prisma/schema.prisma",
    "prisma:migrate:deploy": "prisma migrate deploy --schema prisma/schema.prisma",
    "worker:scheduler": "tsx lib/jobs/scheduler.ts",
    "worker:capture": "tsx lib/jobs/capture-worker.ts",
    "bootstrap:state:us": "tsx lib/scripts/bootstrap-state.ts --marketplace US",
    "bootstrap:state:uk": "tsx lib/scripts/bootstrap-state.ts --marketplace UK"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/lib-storage": "^3.943.0",
    "@aws-sdk/s3-request-presigner": "^3.943.0",
    "@prisma/client": "6.19.0",
    "@targon/auth": "workspace:*",
    "@targon/aws-s3": "workspace:*",
    "@targon/logger": "workspace:*",
    "@targon/prisma-argus": "workspace:*",
    "@targon/prisma-talos": "workspace:*",
    "cheerio": "^1.0.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.559.0",
    "mime-types": "^3.0.2",
    "next": "16.0.8",
    "next-auth": "5.0.0-beta.30",
    "nodemailer": "^6.8.0",
    "playwright": "^1.57.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.4.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^6.4.17",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.6",
    "autoprefixer": "^10.4.22",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^9.1.0",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "prisma": "6.19.0",
    "tailwindcss": "^3.4.17",
    "tslib": "^2.7.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
