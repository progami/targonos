{
  "name": "@targon/plutus",
  "version": "1.1.0",
  "private": true,
  "description": "Plutus â€“ finance workspace for custom financials from QuickBooks Online",
  "scripts": {
    "dev": "next dev -p 3012",
    "clean": "rm -rf .next out dist",
    "build": "prisma generate --schema prisma/schema.prisma && next build",
    "start": "next start -p 3012",
    "lint": "eslint app components lib",
    "test": "tsx tests/run.ts",
    "type-check": "prisma generate --schema prisma/schema.prisma && tsc --noEmit",
    "format": "prettier --check \"{app,components,lib}/**/*.{ts,tsx,mdx}\"",
    "format:write": "prettier --write \"{app,components,lib}/**/*.{ts,tsx,mdx}\"",
    "qbo": "tsx scripts/qbo.ts",
    "autopost:check": "tsx scripts/autopost-check.ts",
    "settlements:us:rematch": "tsx scripts/us-settlement-rematch.ts",
    "settlements:us:reconcile:spapi": "tsx scripts/us-settlement-reconcile-spapi.ts",
    "settlements:us:ingest:spapi": "tsx scripts/us-settlement-ingest-spapi.ts",
    "db:generate": "prisma generate --schema prisma/schema.prisma",
    "db:push": "prisma db push --schema prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema prisma/schema.prisma",
    "db:studio": "prisma studio --schema prisma/schema.prisma"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.8",
    "@mui/lab": "7.0.1-beta.22",
    "@mui/material": "^7.3.8",
    "@mui/system": "^7.3.8",
    "@prisma/client": "6.19.0",
    "@tanstack/react-query": "^5.90.12",
    "@tanstack/react-table": "^8.21.3",
    "@targon/amazon-sp-api": "workspace:*",
    "@targon/auth": "workspace:*",
    "@targon/config": "workspace:*",
    "@targon/logger": "workspace:*",
    "@targon/prisma-plutus": "workspace:*",
    "date-fns": "^4.1.0",
    "decimal.js": "^10.5.0",
    "fflate": "^0.8.2",
    "intuit-oauth": "^4.2.2",
    "next": "16.0.8",
    "next-auth": "5.0.0-beta.30",
    "next-themes": "^0.4.6",
    "notistack": "^3.0.2",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "react-hook-form": "^7.68.0",
    "recharts": "^3.5.1",
    "server-only": "^0.0.1",
    "xlsx": "^0.18.5",
    "zod": "^3.24.4",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "@types/react": "19.0.8",
    "@types/react-dom": "19.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.5.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.5.3",
    "prisma": "^6.5.0",
    "tsx": "^4.21.0",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=20.9.0"
  }
}
